{
  "parser": "@babel/eslint-parser",
  "extends": ["react", "plugin:jest-dom/recommended"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "indent": ["error", 2]
  },
  "plugins": ["jest-dom"],
  "globals": {
    "page": true,
    "browser": true,
    "context": true,
    "jestPuppeteer": true
  }
}